[flake8]
ignore=F812,H101,H202,H233,H301,H306,H401,H403,H404,H405,H501,E203,E266,E501,W503,F403,F401,F405,E231,F841,C901,F821,E722,E714,E713,E711,W291,E402,W605,F632
